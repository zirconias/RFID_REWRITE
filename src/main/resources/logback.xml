<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %5p %50.50c:%4L - %m%n</pattern>
        </encoder>
    </appender>

    <logger name="com.icosnet.rfid" level="debug" />

    <root level="warn">
        <appender-ref ref="console" />
    </root>

</configuration>